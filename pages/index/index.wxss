.index {
  padding: 0rpx 18rpx;
}

.banner {
  height: auto;
  position: relative;
}

.banner-swipers {
  width: 100%;
  height: 350rpx;
  text-align: center;
}

.banner-swipers image {
  width: 100%;
  height: 100%;
  -webkit-border-radius: 12rpx;
  -moz-border-radius: 12rpx;
  border-radius: 12rpx;
  -webkit-box-shadow: 0 0 16rpx rgba(0, 0, 0, 0.25);
  -moz-box-shadow: 0 0 16rpx rgba(0, 0, 0, 0.25);
  box-shadow: 0 0 16rpx rgba(0, 0, 0, 0.25);
}

/*用来包裹所有的小圆点  */

.banner-dots {
  width: 750rpx;
  height: 36rpx;
  display: flex;
  flex-direction: row;
  position: absolute;
  left: 0;
  bottom: 58rpx;
}

/*未选中时的小圆点样式 */

.banner-dot {
  width: 16rpx;
  height: 16rpx;
  margin-right: 26rpx;
  background-color: #fff;
  border-radius: 16rpx;
}

/*选中以后的小圆点样式  */

.banner-active {
  width: 32rpx;
  height: 16rpx;
  background-color: #f18d00;
}

.banner .flex {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  display: box;
  flex-wrap: wrap;
}

.banner .alignC {
  /*元素居中*/
  align-items: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -moz-align-items: center;
  -ms-align-items: center;
  -o-align-items: center;
}

.banner .flexC {
  -webkit-box-pack: center;
  justify-content: center;
  -webkit-justify-content: center;
  -moz-justify-content: center;
  -ms-justify-content: center;
  -o-justify-content: center;
}

.fc {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  padding: 18rpx 0rpx;
  display: flex;
  flex-direction: row;
  -webkit-border-radius: 12rpx;
  -moz-border-radius: 12rpx;
  border-radius: 12rpx;
  -webkit-box-shadow: 0 0 16rpx rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 0 16rpx rgba(0, 0, 0, 0.1);
  box-shadow: 0 0 16rpx rgba(0, 0, 0, 0.1);
}

.fc .item {
  flex: 1;
  height: 100rpx;
  text-align: center;
}

.fc .item-img {
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
}

.fc .item-title {
  height: 60rpx;
  display: block;
  text-align: center;
  font-size: 24rpx;
}

.tabbar-items-scroll {
  z-index: 9999;
  height: 62rpx;
  background: #fff;
  overflow: hidden;
}

.tabbar-items {
  height: 61rpx;
  line-height: 42rpx;
  width: 100%;
  transition-timing-function: cubic-bezier(0.1, 0.57, 0.1, 1);
  transition-duration: 0ms;
}

.tabbar-items .container{
  display: flex;
}

.tabbar-items .item {
  height: 60rpx;
  line-height: 42rpx;

  flex: 1;

  overflow: hidden;
  background: #fff;
  text-align: center;
  font-size: 28rpx;
  z-index: 999;
}

.tabbar-contents {
}

.tabbar-items .item-seleced .title {
  display: block;
  margin-bottom: 10rpx;
  color: #ffb101;
}

.tabbar-items .item-seleced .seleced-bottom {
  display: block;
  width: 80rpx;
  text-align: center;
  margin-top: 5rpx;
  line-height: 20rpx;
  background: #ffb101;
  box-shadow: 0px 2px 3px rgba(255, 176, 0, 0.55);
  border-radius: 3px;
  height: 4rpx;
  margin: auto;
}


.item-bannerImgUrl image{
  height: 167rpx;
  width: 100%;
}

.prd-list-1 {
  position: relative;
  overflow: hidden;
}

.prd-list-1 .item {
  position: relative;
  float: left;
  width: 50%;
  box-sizing: border-box;
  text-align: center;
  padding-top: 10px;
  padding-bottom: 10px;
}

.prd-list-1 .item-0 {
  padding-left: 20px;
  padding-right: 10px;
}

.prd-list-1 .item-1 {
  padding-left: 10px;
  padding-right: 20px;
}

.prd-list-1 .item-imgbox {
  text-align: center;
  height: 320rpx;
}

.prd-list-1 .item-imgurl {
  width: 100%;
  height: 100%;
}

.prd-list-1 .item-name {
  width: 100%;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 16px;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.prd-list-1 .item-price {
  width: 100%;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 18px;
  display: block;
  text-align: left;
}
