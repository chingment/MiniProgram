.prd-list-1 .item-imgbox {
  text-align: center;
  height: 320rpx;
}

.prd-list-1 .item-imgurl {
  width: 100%;
  height: 100%;
}

.prd-list-1 .item-name {
  width: 100%;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 16px;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.prd-list-1 .item-price {
  width: 100%;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 18px;
  display: block;
  text-align: left;
}


